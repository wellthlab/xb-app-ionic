{
	"appId": "uk.ac.soton.ecs.xbapp",
	"appName": "xbapp",
	"webDir": "build",
	"bundledWebRuntime": false,
	"plugins": {
		"SplashScreen": {
			"launchAutoHide": false
		},
		"LocalNotifications": {
			"smallIcon": "ic_stat_icon_config_sample",
			"iconColor": "#CE0B7C"
		},
		"PushNotifications": {
			"presentationOptions": [
				"alert",
				"sound"
			]
		}
	},
	"android": {
		"allowMixedContent": true,
		"webContentsDebuggingEnabled": true
	},
	"cordova": {}
}
